{
  "name": "@sapling/cli",
  "description": "CLI for setting up and managing Sapling projects",
  "version": "0.1.0",
  "author": "@groenroos",
  "bin": {
    "sapling": "./bin/run"
  },
  "bugs": "https://github.com/saplingjs/cli/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "@oclif/plugin-not-found": "^1.2.4",
    "@oclif/plugin-warn-if-update-available": "^1.7.0",
    "@types/edit-json-file": "^1.4.0",
    "@types/fs-extra": "^9.0.4",
    "@types/get-port": "^4.2.0",
    "@types/inquirer": "^7.3.1",
    "cli-ux": "^5.5.1",
    "edit-json-file": "^1.5.0",
    "execa": "^4.1.0",
    "fs-extra": "^9.0.1",
    "get-port": "^5.1.1",
    "inquirer": "^7.3.3",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@types/node": "^10",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "globby": "^10",
    "ts-node": "^8",
    "typescript": "^4.1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/saplingjs/cli",
  "keywords": [
    "oclif",
    "cli",
    "sapling"
  ],
  "license": "MPL-2.0",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sapling",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found",
      "@oclif/plugin-warn-if-update-available"
    ]
  },
  "repository": "saplingjs/cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
